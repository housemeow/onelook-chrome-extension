{
  "manifest_version": 2,
  "name": "Onelook",
  "description": "A chrome extension can look up the text you selected via onelook dictionary",
  "version": "1.0",
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background":{
    "scripts": ["onelook.js", "background.js"]
  },
  "icons" :{
    "16": "onelook.png"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}